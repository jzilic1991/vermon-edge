EXISTS mttf, mttr, downtime, totaltime, uptime .
(totaltime <- CNT i;host
	ONCE[0,10s] status (host, indicator)
		AND ts(i))
AND
(uptime <- SUM indicator;host
	ONCE[0,10s] status (host, indicator)
		AND ts(i))
AND
(downtime = totaltime - uptime)
AND
(mttf = i2f(uptime) / i2f(totaltime))
AND
(mttr = i2f(downtime) / i2f(totaltime))
AND
(avail = mttf / (mttf + mttr))
AND
avail < 0.99